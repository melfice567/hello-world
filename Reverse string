import java.util.*; //needed for string entry etc

public class Program {
    public static void main(String[] args) {
        String string;
        String reverse = (""); //need to set value for string before use
        
        string = ("Hello");
        
        
        for(int i = string.length() - 1; i >= 0; i--) //use int to measure length of string, count back while length > 0 and extract char at
        {
            reverse = reverse + string.charAt(i);
        }
        
        System.out.println(reverse);
    }
}
